{
  "parser": "babel-eslint",
  "extends": [
    "standard",
    "standard-react"
  ],
  "plugins": [
    "babel",
    "react",
    "promise",
    "mocha"
  ],
  "env": {},
  "rules": {
    "semi": [
      2,
      "never"
    ],
    "max-len": [
      2,
      120,
      2
    ],
    "generator-star-spacing": 0,
    "babel/generator-star-spacing": 1,
    "jsx-quotes": [
      2,
      "prefer-single"
    ],
    "space-before-function-paren": 0,
    "react/jsx-curly-spacing": 0,
    "mocha/no-exclusive-tests": "error"
  }
}